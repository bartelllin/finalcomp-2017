<script>
  var app = angular.module('finalComp', []);
  app.controller('SuperCtrl', function($scope, $http) { 
    $http.get("https://spreadsheets.google.com/feeds/list/1efUMXdadlgB2Vr4ITq_RkECUX0f_DpNkPTuj6xL3bUE/ovml8vg/public/values?alt=json")
            .success(function(response) {$scope.teams = response.feed.entry;});
            });
</script>

<body ng-cloak ng-app="finalComp" ng-control="SuperCtrl">
  <div flex-gt-sm="100" flex="">
    <md-toolbar layout="row" class="md-hue-3">
      <div class="md-toolbar-tools">
        <span>Teams</span>
      </div>
    </md-toolbar>

    <md-content>
      <md-list flex="">
        <div class="md-list-item-text" layout="column">
          <h3>{{ teams.gsx$teamname.$t }}</h3> 
        </div>
      </md-list>
    </md-content>
  </div>


